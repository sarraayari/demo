<mvc:View controllerName="project1.controller.View1"
    xmlns:mvc="sap.ui.core.mvc" displayBlock="true"  
     xmlns="sap.ui.table"
	xmlns:u="sap.ui.unified"
	xmlns:c="sap.ui.core"
	xmlns:m="sap.m"
    height="100%">
    <m:Page id="page" title="{i18n>title}">
	<m:IconTabBar
                            id="idIconTabBarNoIcons"
                       
                            class="sapUiResponsiveContentPadding">
		<m:items>
			<m:IconTabFilter   key="info" text="SAP HANA Mara">
<m:VBox >
	<Table
    
				rows="{odataModel>/results}"
				selectionMode="MultiToggle"
				paste="onPaste"
				ariaLabelledBy="title">
				<extension>
					<m:OverflowToolbar  style="Clear">
						<m:Title  text="List of Materials"/>
					</m:OverflowToolbar>
				</extension>
				<columns>
					<Column  width="11rem">
						<m:Label  text="Material" />
						<template>
							<m:Text  text="{odataModel>Matnr}" wrapping="false" />
						</template>
					</Column>
					<Column  width="11rem">
						<m:Label  text="Created by" />
						<template>
							<m:Text text="{odataModel>Ernam}" />
						</template>
					</Column>
                    <Column  width="11rem">
						<m:Label  text="Base Unit of Measure" />
						<template>
							<m:Text  text="{odataModel>Meins}"/>
						</template>
					</Column>
                     <Column  width="11rem">
						<m:Label  text="Industry Sector" />
						<template>
							<m:Text  text="{odataModel>Mbrsh}"/>
						</template>
					</Column>
                     <Column  width="11rem">
						<m:Label text="Material type" />
						<template>
							<m:Text text="{odataModel>Mtart}"/>
						</template>
					</Column>
				</columns>
			</Table>
		<m:Button text="Send to Cloud database" press="onPress" />


</m:VBox>
</m:IconTabFilter>
